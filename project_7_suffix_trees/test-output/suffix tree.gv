// Suffix tree
digraph {
	"|" [label=""]
	"t|t" [label=t]
	"at|tat" [label=tat]
	"at$|tatat$" [label="tatat$"]
	"at|tat" -> "at$|tatat$" [label="at$"]
	"$|tat$" [label="tat$"]
	"at|tat" -> "$|tat$" [label="$"]
	"t|t" -> "at|tat" [label=at]
	"$|t$" [label="t$"]
	"t|t" -> "$|t$" [label="$"]
	"|" -> "t|t" [label=t]
	"at|at" [label=at]
	"at$|atat$" [label="atat$"]
	"at|at" -> "at$|atat$" [label="at$"]
	"$|at$" [label="at$"]
	"at|at" -> "$|at$" [label="$"]
	"|" -> "at|at" [label=at]
	"$|$" [label="$"]
	"|" -> "$|$" [label="$"]
}
