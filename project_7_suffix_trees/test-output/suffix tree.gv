// Suffix tree
digraph {
	"|" [label=""]
	"t|t" [label=t]
	"a|ta" [label=ta]
	"t|tat" [label=tat]
	"a|tata" [label=tata]
	"t|tatat" [label=tatat]
	"$|tatat$" [label="tatat$"]
	"t|tatat" -> "$|tatat$" [label="$"]
	"a|tata" -> "t|tatat" [label=t]
	"$|tata$" [label="tata$"]
	"a|tata" -> "$|tata$" [label="$"]
	"t|tat" -> "a|tata" [label=a]
	"a|ta" -> "t|tat" [label=t]
	"t|t" -> "a|ta" [label=a]
	"|" -> "t|t" [label=t]
}
